# build the simple color crate
crate  =simcolor
crate main=lib
dep_crates=[]
comp opts=[]
crate_src=src
test file=test
partial=true
common =..${~/~}simscript${~/~}comm-crate.7b:file
common_test =..${~/~}simscript${~/~}comm-test.7b:file
crate_dir=..${~/~}crates

version=1.03b007
set_env(VERSION,version)
if {
	eq(partial,true) then {
		array(--cfg, "partial_reset")
		assign(comp opts,~~)
	}
}
include(common);
include(common_test);